package methodTest;

public class MethodP {
	
	void increase(int data) {
		data+= 10;
//		
	}
	
	void increase2(int[] data) {
		data[0] +=10;
	}
	
	
	public static void main(String[] args) {
		MethodP m = new MethodP();
		int data = 20;
		m.increase(data);
		System.out.println(data);
		
		
		
		int[] data2 = {20};
		m.increase2(data2);
		System.out.println(data2);
	}
}
